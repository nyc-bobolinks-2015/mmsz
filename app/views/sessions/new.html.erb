<%= form_tag sessions_path do %>
  <fieldset>
    <div>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </div>
    <div>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div>
      <%= submit_tag 'Log in' %>
    </div>
  </fieldset>
<% end %>
